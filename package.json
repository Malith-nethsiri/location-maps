{
  "name": "location-intelligence-app",
  "version": "1.0.0",
  "description": "Comprehensive location intelligence web application with GPS coordinate analysis, POI discovery, navigation, and satellite imagery",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "echo 'No root build needed - backend handles its own build'",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Malith-nethsiri/location-maps.git"
  },
  "keywords": [
    "location",
    "intelligence",
    "gps",
    "coordinates",
    "maps",
    "navigation",
    "poi",
    "satellite",
    "react",
    "nodejs",
    "postgresql",
    "google-maps",
    "valuation",
    "sri-lanka"
  ],
  "author": "Malith Nethsiri",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {}
}
