{
  "version": 2,
  "name": "location-intelligence-app",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://location-intelligence-backend-production.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://location-intelligence-backend-production.railway.app"
  },
  "build": {
    "env": {
      "REACT_APP_API_BASE_URL": "https://location-intelligence-backend-production.railway.app"
    }
  }
}